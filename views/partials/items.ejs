<section>
  <div class="flex gap-10 text-white flex-wrap">
    <% console.log(allItems); %>
    <% allItems.forEach((item) => { %>
      <div class="flex flex-col items-center gap-5 border-2 border-dashed border-secondary p-5 rounded-3xl w-48">
        <h1 class="font-mono text-xl "><%= item.item_name %></h1>
        <div class="flex gap-5 items-center justify-center">
          <button class=" text-sm font-bold text-accent ">Edit</button>
          <button class=" text-sm font-bold text-red-500 ">Delete</button>
        </div>
      </div>
    <% }) %>
  </div>
</section>
