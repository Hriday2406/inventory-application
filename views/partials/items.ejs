  <div class="flex gap-5 sm:gap-10 text-white flex-wrap">
    <% allItems.forEach((item) => { %>
      <div class="flex flex-col items-center overflow-hidden gap-5 border-2 border-dashed border-secondary p-5 rounded-3xl w-44">
        <h1 class="font-mono text-xl text-pretty  "><%= item.item_name %></h1>
        <div class="flex gap-5 items-center justify-center">
          <a href="/item/edit/<%= item.id %>" class=" text-sm font-bold text-accent select-none">Edit</a>
          <a href="/item/delete/<%= item.id %>" class=" text-sm font-bold text-red-500 select-none">Delete</a>
        </div>
      </div>
    <% }) %>
  </div>
